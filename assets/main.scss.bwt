	&:hover{
		@media(min-width: 1200px){
			.product-thumbnail {
				.product_overlay {
					opacity:.5;
				}
				.xem_nhanh{
					opacity:1;
				}
				.image_thumb{
					&:before{
						opacity:1;
						z-index: 9;
					}
				}
			}
			.product-action{
				-moz-transform: scale(1);
				-o-transform: scale(1);
				-ms-transform: scale(1);
				-webkit-transform: scale(1);
				transform: scale(1);
			}

		}
	}
